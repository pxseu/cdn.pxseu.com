{
    "compilerOptions": {
        "target": "es2018",
        "module": "commonjs",
        "outDir": "dist",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "allowJs": false,
        "skipLibCheck": true,
        "strict": false,
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "src/**/*"
    ]
}